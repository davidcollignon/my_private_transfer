<%= simple_form_for [@car, @service] do |f| %>

  <%= f.input :flight_number, label: "Flight Number" %>
  <%= f.input :driver_language, label: "Driver's Language"%>
  <%= f.input :number_of_passengers, label: "Number of passengers" %>
  <%= f.input :number_normal_luggage, label: "Luggage" %>
  <%= f.input :number_hand_luggage, label: "Hand Luggage" %>
  <%= f.input :number_odd_luggage, label: "Odd Luggage" %>
  <%= f.input :description_odd_luggage, label: "Description of odd luggage"%>
  <%= f.input :additional_info, label: "Addtional Information"%>
  <%= f.input :commission_rate, label: "Commission" %>
  <%= f.submit "Continue", class: "btn btn-primary" %>
 <% end %>
